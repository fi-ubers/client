<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp"
        android:left="8dp">
        <shape
            android:shape="oval">
            <solid android:color="#335676"
                android:alpha="0.9"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="oval">
            <stroke android:width="4dp"
                    android:color="#415071"/>
        </shape>
    </item>

</layer-list>

